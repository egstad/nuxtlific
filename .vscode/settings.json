{
  "vetur.validation.template": false,
  "editor.formatOnSave": false,
  "eslint.autoFixOnSave": true,
  "prettier.eslintIntegration": true,
  "eslint.validate": [
    {
      "language": "vue",
      "autoFix": true
    },
    {
      "language": "json",
      "autoFix": true
    },
    {
      "language": "javascript",
      "autoFix": true
    },
    {
      "language": "javascriptreact",
      "autoFix": true
    },
    {
      "language": "scss",
      "autoFix": true
    },
    {
      "language": "md",
      "autoFix": true
    },
  ],
  "files.associations": {
    "*.css": "vue-postcss"
  }
}
